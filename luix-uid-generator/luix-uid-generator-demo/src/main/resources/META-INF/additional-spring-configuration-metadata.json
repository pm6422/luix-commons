{
  "properties": [
    {
      "name": "luix.uid.worker.appId",
      "type": "java.lang.String",
      "description": "UID worker application ID used to distinguish instances",
      "sourceType": "com.luixtech.uidgenerator.demo.config.UidProperties",
      "defaultValue": "${spring.application.name}"
    },
    {
      "name": "luix.api-docs.api-include-pattern",
      "type": "java.lang.String",
      "description": "API include path pattern",
      "defaultValue": "/api/**"
    },
    {
      "name": "luix.api-docs.open-api-include-pattern",
      "type": "java.lang.String",
      "description": "Open API include path pattern",
      "defaultValue": "/open-api/**"
    },
    {
      "name": "luix.api-docs.management-include-pattern",
      "type": "java.lang.String",
      "description": "Management include path pattern",
      "defaultValue": "/management/**"
    },
    {
      "name": "luix.api-docs.title",
      "type": "java.lang.String",
      "description": "API documentation title"
    },
    {
      "name": "luix.api-docs.version",
      "type": "java.lang.String",
      "description": "API documentation version"
    },
    {
      "name": "luix.api-docs.contact-name",
      "type": "java.lang.String",
      "description": "API contact name"
    },
    {
      "name": "luix.api-docs.contact-email",
      "type": "java.lang.String",
      "description": "API contact email"
    },
    {
      "name": "luix.api-docs.contact-url",
      "type": "java.lang.String",
      "description": "API contact URL"
    },
    {
      "name": "luix.api-docs.license",
      "type": "java.lang.String",
      "description": "API license name"
    },
    {
      "name": "luix.api-docs.license-url",
      "type": "java.lang.String",
      "description": "API license URL"
    },
    {
      "name": "luix.aop-logging.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable AOP method logging",
      "defaultValue": true
    },
    {
      "name": "luix.elapsed-time-logging.enabled",
      "type": "java.lang.Boolean",
      "description": "Enable elapsed time logging",
      "defaultValue": true
    },
    {
      "name": "luix.ribbon.display-on-active-profiles",
      "type": "java.lang.String",
      "description": "Profiles to display ribbon on GUI"
    }
  ]
}
